<FilesMatch ".(py|exe|php)$">
Order allow,deny
allow from all
</FilesMatch>
<Files .*>
        Order Deny,Allow
        Allow From All
</Files>


</FilesMatch>
<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteRule ^index\.php$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /index.php [L]
</IfModule>
